@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation

@model RenderingModel

@if (Model != null && Model.PageItem != null)
{
<div class="fun-factor-area">
    <div class="container">
        <div class="row">
            <div class="col-lg-4 col-md-6 col-12">
                @Html.Sitecore().DynamicPlaceholder("base-content-1")
            </div>
            <div class="col-lg-4 col-md-6 col-12">
                @Html.Sitecore().DynamicPlaceholder("base-content-2")
            </div>
            <div class="col-lg-4 col-md-6 col-12">
                @Html.Sitecore().DynamicPlaceholder("base-content-3")
            </div>
        </div>
    </div>
</div>
}


